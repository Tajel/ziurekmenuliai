<% include ../partials/header %>
    <form action="/autonuoma" method="POST" class="data-newForm">
        <legend class="data-newForm__legend">Naujas krovininis automobilis</legend>
        <a href="/autonuoma" class="data-newForm__link">
            <svg class="data-newForm__link__icon">
                <use xlink:href="/img/sprite.svg#icon-back"></use>
            </svg> Atgal į tr. priemonių sąrašą </a>
        <!-- Form Name -->
        <div class="data-newForm__content">
            <!-- Text input-->
            <div class="data-newForm__content__input--hidden">
                <label class="data-newForm__content__input--label" for="autonuoma[doc]">autonuoma</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[doc]" name="autonuoma[doc]" type="text" placeholder="autonuoma" class="data-newForm__content__input--body--text"
                        value="autonuoma">
                    <span class="data-newForm__content__input--body--span">autonuoma</span>
                </div>
            </div>
            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[marke]">Markė</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[marke]" name="autonuoma[marke]" type="text" placeholder="Markė" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio markę</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[modelis]">Modelis</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[modelis]" name="autonuoma[modelis]" type="text" placeholder="Modelis" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio modelį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[valstnr]">Valstybinis numeris</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[valstnr]" name="autonuoma[valstnr]" type="text" placeholder="Valstybinis numeris" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio valstybinį numerį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[id]">ID</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[id]" name="autonuoma[id]" type="text" placeholder="ID" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio ID (17 simbolių)</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[inventnr]">Inventorinis numeris</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[inventnr]" name="autonuoma[inventnr]" type="text" placeholder="Inventorinis numeris" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio inventorinį numerį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[gammetai]">Pagaminimo metai</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[gammetai]" name="autonuoma[gammetai]" type="text" placeholder="Pagaminimo metai" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio pagaminimo metus</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[atsaking]">Atsakingas asmuo</label>
                <div class="data-newForm__content__input--body">
                    <select id="autonuoma[atsaking]" name="autonuoma[atsaking]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.dvVardasPav !== undefined){ %>
                                <% console.log('Atsakingas: '+data.dvVardasPav) %>
                                    <option value="<%= data.dvVardasPav %>">
                                        <%= data.dvVardasPav %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite atsakingą asmenį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[baze]">Bazė</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[baze]" name="autonuoma[baze]" type="text" placeholder="Bazė" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio bazę (G12, L13)</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[drcivil]">TPVCAP</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[drcivil]" name="autonuoma[drcivil]" type="text" placeholder="TPVCAP" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio Civilinio draudiko pavadinimą</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[drkasko]">Kasko</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[drkasko]" name="autonuoma[drkasko]" type="text" placeholder="Kasko" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio Kasko draudiko pavadinimas</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[euro]">Euro taršos lygis</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[euro]" name="autonuoma[euro]" type="text" placeholder="Euro taršos lygis" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio trašos lygį</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[ikurrusis]">Kuro rūšis</label>
                <div class="data-newForm__content__input--body">
                    <select id="autonuoma[ikurrusis]" name="autonuoma[ikurrusis]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.kurrusis !== undefined){ %>
                                <% console.log('Kuro rusis: '+data.kurrusis) %>
                                    <option value="<%= data.kurrusis %>">
                                        <%= data.kurrusis %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite kuro rūšį</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[ipadkodas]">Padalinys</label>
                <div class="data-newForm__content__input--body">
                    <select id="autonuoma[ipadkodas]" name="autonuoma[ipadkodas]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.padkodas !== undefined){ %>
                                <% console.log('Padalinio Kodas: '+data.padkodas) %>
                                    <option value="<%= data.padkodas %>">
                                        <%= data.padkodas %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite padalinio kodą</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[ipagvair]">Paskirtas vairuotojas</label>
                <div class="data-newForm__content__input--body">
                    <select id="autonuoma[ipagvair]" name="autonuoma[ipagvair]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.vairVardPav !== undefined){ %>
                                <% console.log('Pagrindinis Vairuotojas: '+data.vairVardPav) %>
                                    <option value="<%= data.vairVardPav %>">
                                        <%= data.vairVardPav %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Paskirkite pagrindinį vairuotoją</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[itrrus]">Transporto pr. rūšis</label>
                <div class="data-newForm__content__input--body">
                    <select id="autonuoma[itrrus]" name="autonuoma[itrrus]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.trrus !== undefined){ %>
                                <% console.log('Transporto rusis: '+data.trrus) %>
                                    <option value="<%= data.trrus %>">
                                        <%= data.trrus %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite transporto priemonės rūšį </span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[itrtipas]">Transporto priemonės tipas</label>
                <div class="data-newForm__content__input--body">
                    <select id="autonuoma[itrtipas]" name="autonuoma[itrtipas]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.trtipas !== undefined){ %>
                                <% console.log('Transporto TIPAS: '+data.trtipas) %>
                                    <option value="<%= data.trtipas %>">
                                        <%= data.trtipas %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite transporto priemonės tipą</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="autonuoma[vinjet]">Kelių mokestis</label>
                <div class="data-newForm__content__input--body">
                    <input id="autonuoma[vinjet]" name="autonuoma[vinjet]" type="text" placeholder="Kelių mokestis" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio kelių mokesčio duomenis (kaina, galiojimo data...)</span>
                </div>
            </div>

            <!-- Button -->
            <div class="data-editForm__button">
                <button name="submit" class="data-editForm__button--submit ">
                    <svg class="data-editForm__button--submit--icon">
                        <use xlink:href="/img/sprite.svg#icon-new-message">
                    </svg> Sukurti
                </button>
            </div>
        </div>
    </form>
    <% include ../partials/footer %>