<% include ../partials/header %>
    <div class="container">
        <header class="jumbotron">
            <h1 style="text-align: center">Redaguoti
                <%= autonuoma.autolen %>
                    <%= autonuoma.modelis %>
                        <%= autonuoma.valstnr %>
            </h1>
        </header>
    </div>
    <div class="container">
        <div style="width: 40%; margin: 35px auto;">
            <form action="/autonuoma/<%= autonuoma._id %>?_method=PUT" method="POST">
                <div class="form-group" hidden>
                    <input class="form-control" type="text" name="autonuoma[doc]" value="autonuoma">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[marke]" placeholder="markė" value="<%= autonuoma.marke %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[imone]" placeholder="imone" value="<%= autonuoma.imone %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[modelis]" placeholder="modelis" value="<%= autonuoma.modelis %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[valstnr]" placeholder="valstybinis numeris" value="<%= autonuoma.valstnr %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[id]" placeholder="id" value="<%= autonuoma.id %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[inventnr]" placeholder="inventorinis numeris" value="<%= autonuoma.inventnr %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[gammetai]" placeholder="gamybos metai" value="<%= autonuoma.gammetai %>">
                </div>
                <div class="form-group">
                    <select style="width: 100%; height: 34px" name="autonuoma[atsaking]" class="custom-select">
                        <option selected>
                            <%= autonuoma.atsaking %>
                        </option>
                        <% data.forEach(function(data){ %>
                            <% if(data.vairVardPav !== undefined){ %>
                                <option>
                                    <%= data.vairVardPav %>
                                </option>
                                <!-- ########## reikia sukurti vairuotojus *********** -->
                                <% } %>
                                    <% }); %>
                    </select>
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[baze]" placeholder="bazė" value="<%= autonuoma.baze %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[drcivil]" placeholder="draudimas civilinis" value="<%= autonuoma.drcivil %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[drkasko]" placeholder="draudimas kasko" value="<%= autonuoma.drkasko %>">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="autonuoma[euro]" placeholder="euro taršos lygis" value="<%= autonuoma.euro %>">
                </div>
                <div class="form-group">
                    <select style="width: 100%; height: 34px" name="autonuoma[ikurrusis]" class="custom-select">
                        <option selected>
                            <%= autonuoma.ikurrusis %>
                        </option>
                        <% data.forEach(function(data){ %>
                            <% if(data.kurrusis !== undefined){ %>
                                <% console.log(data.kurrusis) %>
                                    <option>
                                        <%= data.kurrusis %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                </div>
                <div class="form-group">
                    <select style="width: 100%; height: 34px" name="autonuoma[ipadkodas]" class="custom-select">
                        <option selected>
                            <%= autonuoma.ipadkodas %>
                        </option>
                        <% data.forEach(function(data){ %>
                            <% if(data.padkodas !== undefined){ %>
                                <option>
                                    <%= data.padkodas %>
                                </option>
                                <% } %>
                                    <% }); %>
                    </select>
                </div>
                <div class="form-group">
                    <select style="width: 100%; height: 34px" name="autonuoma[ipagvair]" class="custom-select">
                        <option selected>
                            <%= autonuoma.ipagvair %>
                        </option>
                        <% data.forEach(function(data){ %>
                            <% if(data.vairVardPav !== undefined){ %>
                                <option>
                                    <%= data.vairVardPav %>
                                </option>
                                <% } %>
                                    <% }); %>
                    </select>
                </div>
                <div class="form-group">
                    <select style="width: 100%; height: 34px" name="autonuoma[itrrus]" class="custom-select">
                        <option selected>
                            <%= autonuoma.itrrus %>
                        </option>
                        <% data.forEach(function(data){ %>
                            <% if(data.trrus !== undefined){ %>
                                <option>
                                    <%= data.trrus %>
                                </option>
                                <% } %>
                                    <% }); %>
                    </select>
                </div>
                <div class="form-group">
                    <select style="width: 100%; height: 34px" name="autonuoma[itrtipas]" class="custom-select">
                        <option selected>
                            <%= autonuoma.itrtipas %>
                        </option>
                        <% data.forEach(function(data){ %>
                            <% if(data.trtipas !== undefined){ %>
                                <option>
                                    <%= data.trtipas %>
                                </option>
                                <% } %>
                                    <% }); %>
                    </select>
                </div>
                <!--<div class="form-group">-->
                <!--    <input class="form-control" type="text" name="autonuoma[vinjet]" placeholder="Kelių mokesčiai" value="<%= autonuoma.vinjet %>">-->
                <!--</div>-->
                <!--<div>-->
                <button class="btn btn-lg btn-primary btn-block">Submit!</button>
        </div>
        </form>
        <a href="/autonuoma">Go Back</a>
    </div>
    </div>

    <% include ../partials/footer %>