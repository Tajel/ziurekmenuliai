<% include ../partials/header %>
    <form action="/automech" method="POST" class="data-newForm">
        <legend class="data-newForm__legend">Naujas mechanizmas</legend>
        <a href="/automech" class="data-newForm__link">
            <svg class="data-newForm__link__icon">
                <use xlink:href="/img/sprite.svg#icon-back"></use>
            </svg> Atgal į mechanizmų sąrašą </a>
        <!-- Form Name -->
        <div class="data-newForm__content">
            <!-- Text input-->
            <div class="data-newForm__content__input--hidden">
                <label class="data-newForm__content__input--label" for="automech[doc]">automech</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[doc]" name="automech[doc]" type="text" placeholder="automech" class="data-newForm__content__input--body--text"
                        value="automech">
                    <span class="data-newForm__content__input--body--span">automech</span>
                </div>
            </div>
            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[marke]">Markė</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[marke]" name="automech[marke]" type="text" placeholder="Markė" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio markę</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[modelis]">Modelis</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[modelis]" name="automech[modelis]" type="text" placeholder="Modelis" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio modelį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[valstnr]">Valstybinis numeris</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[valstnr]" name="automech[valstnr]" type="text" placeholder="Valstybinis numeris" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio valstybinį numerį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[id]">ID</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[id]" name="automech[id]" type="text" placeholder="ID" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio ID (17 simbolių)</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[inventnr]">Inventorinis numeris</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[inventnr]" name="automech[inventnr]" type="text" placeholder="Inventorinis numeris" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio inventorinį numerį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[gammetai]">Pagaminimo metai</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[gammetai]" name="automech[gammetai]" type="text" placeholder="Pagaminimo metai" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio pagaminimo metus</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[atsaking]">Atsakingas asmuo</label>
                <div class="data-newForm__content__input--body">
                    <select id="automech[atsaking]" name="automech[atsaking]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.dvVardasPav !== undefined){ %>
                                <% console.log('Atsakingas: '+data.dvVardasPav) %>
                                    <option value="<%= data.dvVardasPav %>">
                                        <%= data.dvVardasPav %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite atsakingą asmenį</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[baze]">Bazė</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[baze]" name="automech[baze]" type="text" placeholder="Bazė" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio bazę (G12, L13)</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[drcivil]">TPVCAP</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[drcivil]" name="automech[drcivil]" type="text" placeholder="TPVCAP" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio Civilinio draudiko pavadinimą</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[drkasko]">Kasko</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[drkasko]" name="automech[drkasko]" type="text" placeholder="Kasko" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio Kasko draudiko pavadinimas</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[euro]">Euro taršos lygis</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[euro]" name="automech[euro]" type="text" placeholder="Euro taršos lygis" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio trašos lygį</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[ikurrusis]">Kuro rūšis</label>
                <div class="data-newForm__content__input--body">
                    <select id="automech[ikurrusis]" name="automech[ikurrusis]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.kurrusis !== undefined){ %>
                                <% console.log('Kuro rusis: '+data.kurrusis) %>
                                    <option value="<%= data.kurrusis %>">
                                        <%= data.kurrusis %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite kuro rūšį</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[ipadkodas]">Padalinys</label>
                <div class="data-newForm__content__input--body">
                    <select id="automech[ipadkodas]" name="automech[ipadkodas]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.padkodas !== undefined){ %>
                                <% console.log('Padalinio Kodas: '+data.padkodas) %>
                                    <option value="<%= data.padkodas %>">
                                        <%= data.padkodas %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite padalinio kodą</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[ipagvair]">Paskirtas vairuotojas</label>
                <div class="data-newForm__content__input--body">
                    <select id="automech[ipagvair]" name="automech[ipagvair]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.vairVardPav !== undefined){ %>
                                <% console.log('Pagrindinis Vairuotojas: '+data.vairVardPav) %>
                                    <option value="<%= data.vairVardPav %>">
                                        <%= data.vairVardPav %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Paskirkite pagrindinį vairuotoją</span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[itrrus]">Transporto pr. rūšis</label>
                <div class="data-newForm__content__input--body">
                    <select id="automech[itrrus]" name="automech[itrrus]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.trrus !== undefined){ %>
                                <% console.log('Transporto rusis: '+data.trrus) %>
                                    <option value="<%= data.trrus %>">
                                        <%= data.trrus %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite transporto priemonės rūšį </span>
                </div>
            </div>

            <!-- Select Basic -->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[itrtipas]">Transporto priemonės tipas</label>
                <div class="data-newForm__content__input--body">
                    <select id="automech[itrtipas]" name="automech[itrtipas]" class="data-newForm__content__input--body--text">
                        <% data.forEach(function(data){ %>
                            <% if(data.trtipas !== undefined){ %>
                                <% console.log('Transporto TIPAS: '+data.trtipas) %>
                                    <option value="<%= data.trtipas %>">
                                        <%= data.trtipas %>
                                    </option>
                                    <% } %>
                                        <% }); %>
                    </select>
                    <span class="data-newForm__content__input--body--span">Pasirinkite transporto priemonės tipą</span>
                </div>
            </div>

            <!-- Text input-->
            <div class="data-newForm__content__input">
                <label class="data-newForm__content__input--label" for="automech[vinjet]">Kelių mokestis</label>
                <div class="data-newForm__content__input--body">
                    <input id="automech[vinjet]" name="automech[vinjet]" type="text" placeholder="Kelių mokestis" class="data-newForm__content__input--body--text">
                    <span class="data-newForm__content__input--body--span">Įrašykite automobilio kelių mokesčio duomenis (kaina, galiojimo data...)</span>
                </div>
            </div>

            <!-- Button -->
            <div class="data-editForm__button">
                <button name="submit" class="data-editForm__button--submit ">
                    <svg class="data-editForm__button--submit--icon">
                        <use xlink:href="/img/sprite.svg#icon-new-message">
                    </svg> Sukurti
                </button>
            </div>
        </div>
    </form>

    <% include ../partials/footer %>