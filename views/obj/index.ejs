<% include ../partials/header %>

    <div class="page-header">
        <h1>OBJEKTAI
            <small>Sukurti naujÄ…, redaguoti, trinti</small>
            <p>
        </h1>
        <a class="btn btn-primary btn-large" href="/obj/new">Naujas objektas</a>
        </p>
    </div>
    <div class="row">
        <% allobjekt.forEach(function(allobj){ %>
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <div class="caption">
                        <h3>
                            <strong>
                                <%= allobj.objpav %>
                            </strong>
                        </h3>
                        <h4>
                            <p>Objekto kodas:
                                <%= allobj.objkodas %>
                            </p>
                            <p> Pagr. DV:
                                <%= allobj.dvVadasPav %>
                            </p>
                        </h4>
                        <p>
                            <a class="btn btn-primary" href="/obj/<%= allobj._id%>/edit">REDAGUOTI</a>

                            <form id="delete-form" action="/obj/<%=allobj._id%>?_method=DELETE" , method=POST>
                                <button class="btn btn-danger">TRINTI</button>
                            </form>
                        </p>
                    </div>
                </div>
            </div>
            <% }); %>
    </div>
    <% include ../partials/footer %>