<% include ../partials/header %>
<div class="container">
    <header class="jumbotron">
        <h1 style="text-align: center">Redaguoti <%= autolen.autolen %>   <%= autolen.modelis %>   <%= autolen.valstnr %></h1>
    </header>
</div>
<div class="container">
    <div style="width: 40%; margin: 35px auto;">
        <form action="/autolen/<%= autolen._id %>?_method=PUT" method="POST">
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[doc]" value="autokrov" readonly>
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[marke]" placeholder="markė" value="<%= autolen.marke %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[modelis]" placeholder="modelis" value="<%= autolen.modelis %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[valstnr]" placeholder="valstybinis numeris" value="<%= autolen.valstnr %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[id]" placeholder="id" value="<%= autolen.id %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[inventnr]" placeholder="inventorinis numeris" value="<%= autolen.inventnr %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[gammetai]" placeholder="gamybos metai" value="<%= autolen.gammetai %>">
            </div>
            <div class="form-group">
                <select style="width: 100%; height: 34px" name="autolen[atsaking]" class="custom-select">
                 <option  selected><%= autolen.atsaking %></option> 
                <% data.forEach(function(data){ %>
                    <% if(data.vairVardPav !== undefined){ %>
                    <option><%= data.vairVardPav %></option>  <!-- ########## reikia sukurti vairuotojus *********** -->
                    <% } %>
                <% }); %>
            </select>
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[baze]" placeholder="bazė" value="<%= autolen.baze %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[drcivil]" placeholder="draudimas civilinis" value="<%= autolen.drcivil %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[drkasko]" placeholder="draudimas kasko" value="<%= autolen.drkasko %>">
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[euro]" placeholder="euro taršos lygis" value="<%= autolen.euro %>">
            </div>
            <div class="form-group">
                <select style="width: 100%; height: 34px" name="autolen[ikurrusis]" class="custom-select">
                 <option selected><%= autolen.ikurrusis %></option> 
                <% data.forEach(function(data){ %>
                    <% if(data.kurrusis !== undefined){ %>
                    <% console.log(data.kurrusis) %>
                    <option><%= data.kurrusis %></option>
                    <% } %>
                <% }); %>
            </select>
            </div>
            <div class="form-group">
                <select style="width: 100%; height: 34px" name="autolen[ipadkodas]" class="custom-select">
                 <option  selected><%= autolen.ipadkodas %></option> 
                <% data.forEach(function(data){ %>
                    <% if(data.padkodas !== undefined){ %>
                    <option><%= data.padkodas %></option>
                    <% } %>
                <% }); %>
            </select>
            </div>
            <div class="form-group">
                <select style="width: 100%; height: 34px" name="autolen[ipagvair]" class="custom-select">
                 <option  selected><%= autolen.ipagvair %></option> 
                <% data.forEach(function(data){ %>
                    <% if(data.vairVardPav !== undefined){ %>
                    <option><%= data.vairVardPav %></option>
                    <% } %>
                <% }); %>
            </select>
            </div>
            <div class="form-group">
                <select style="width: 100%; height: 34px" name="autolen[itrrus]" class="custom-select">
                 <option  selected><%= autolen.itrrus %></option> 
                <% data.forEach(function(data){ %>
                    <% if(data.trrus !== undefined){ %>
                    <option><%= data.trrus %></option>
                    <% } %>
                <% }); %>
            </select>
            </div>
            <div class="form-group">
                <select style="width: 100%; height: 34px" name="autolen[itrtipas]" class="custom-select">
                 <option  selected><%= autolen.itrtipas %></option> 
                <% data.forEach(function(data){ %>
                    <% if(data.trtipas !== undefined){ %>
                    <option><%= data.trtipas %></option>
                    <% } %>
                <% }); %>
            </select>
            </div>
            <div class="form-group">
                <input class="form-control" type="text" name="autolen[vinjet]" placeholder="Kelių mokesčiai" value="<%= autolen.vinjet %>">
            </div>
            <div>
                <button class="btn btn-lg btn-primary btn-block">Submit!</button>
            </div>
        </form>
        <a href="/autolen">Go Back</a>
    </div>
</div>

<% include ../partials/footer %>

