<% include ../partials/header %>
    <% data.forEach(function(data1){ %>
        <form action="/kellap" method="POST" onsubmit="button.disabled = true; return true;">
            <input class="form-control" type="text" name="kellap[doc]" value="<%= data1.itrrus %>" hidden>
            <input class="form-control" type="text" name="kellap[kellap]" value="kellapas" hidden>
            <input class="form-control" type="text" name="kellap[autoObjectId]" value="<%= data1._id %>" hidden>
            <input class="form-control" type="text" name="kellap[baze]" value="<%= data1.baze %>" hidden>
            <input style="display:inline" type="text" name="kellap[marke]" value="<%= data1.marke %>" readonly>
            <input style="display:inline" type="text" name="kellap[modelis]" value="<%= data1.modelis %>" readonly>
            <input style="display:inline" type="text" name="kellap[valstnr]" value="<%= data1.valstnr %>" readonly>
            <input style="display:inline" type="text" name="kellap[ipagvair]" value="<%= data1.ipagvair %>" readonly>
            <input style="display:inline" type="text" name="kellap[inventnr]" value="<%= data1.inventnr %>" readonly>
            <input style="display:inline" type="text" name="kellap[itrrus]" value="<%= data1.itrrus %>" readonly>
            <input style="display:inline" type="text" name="kellap[ikurrusis]" value="<%= data1.ikurrusis %>" readonly>
            <input style="display:inline" type="text" name="kellap[data]" placeholder="2017.01.01">
            <input class="btn btn-sm btn-primary" name='button' type="submit" value="OK">
        </form>
        <% }); %>
            <% include ../partials/footer %>